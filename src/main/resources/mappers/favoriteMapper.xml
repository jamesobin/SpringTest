<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.jamesobin.spring.test.ajax.repository.FavoriteRepository">
 	<select id="selectFavorite" resultType="com.jamesobin.spring.test.ajax.domain.Favorite">
 		SELECT
 			`id`
 			, `name`
 			, `url`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`favorite`
 	</select>
	<insert id="insertFavorite" parameterType="map">
		INSERT INTO
			`favorite`
		(
			`name`
			, `url`
		)
		VALUE
		(
			#{name}
			, #{url}
		)
	</insert>
 </mapper>